{
    "name": "know-that/laravel-logger",
    "description": "The know-that/laravel-logger is the view page of the larravel log.",
    "keywords": [
        "laravel",
        "log",
        "view",
        "log view"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "wangfeiyang",
            "email": "knowthat@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=5.8"
    },
    "autoload": {
        "psr-4": {
            "Knowthat\\LaravelLogger\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "KnowThat\\LaravelLogger\\LoggerProvider"
            ],
            "aliases": {
                "Debugbar": "KnowThat\\LaravelLogger\\Facades"
            }
        }
    }
}
